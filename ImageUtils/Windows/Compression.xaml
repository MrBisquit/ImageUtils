<Window x:Class="ImageUtils.Windows.Compression"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImageUtils.Windows"
        mc:Ignorable="d"
        Title="Compression" Height="350" Width="500"
        Loaded="Window_Loaded"
        WindowStartupLocation="CenterOwner">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
        </Grid.RowDefinitions>

        <WrapPanel Grid.Row="1" HorizontalAlignment="Right" Margin="6">
            <DockPanel>
                <Button DockPanel.Dock="Left" Width="100" Name="CancelBtn" Click="CancelBtn_Click">Cancel</Button>
                <Label></Label> <!-- Make-do spacing because spacing doesn't exist for some reason lol -->
                <Button DockPanel.Dock="Right" Width="100">Finish</Button>
            </DockPanel>
        </WrapPanel>
    </Grid>
</Window>
